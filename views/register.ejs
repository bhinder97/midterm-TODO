<body>
  <container class="parent">
   <nav>
      <span class="nav-span">
          <div class="tittle">ToDo</div>
          <br>
      </span>
    </nav>
      <form class="register-post" method="POST" action="/register">
        <label class="name-label">
          Name
          <input class="name-input" type="name" name="name" />
        </label>
        <label class="email-label">
          Email
          <input class="email-input" type="email" name="email" />
        </label>
        <label class="password-label">
          Password
          <input class="password-input" type="password" name="password" />
        </label>
        <button class="register-button" type="submit">Register</button>
      </form>
  </container>
</body>
